<div class="tab-pane fade" id="services" role="tabpanel">
    <div class="container">
        <div class="row">
            <div class="col-12 col-md-8 offset-md-2">
                <table class="table table-responsive table-hover">
                    <thead>
                        <tr>
                            <th>Servicio</th>
                            <th class="text-center">¿De qué trata?</th>
                        </tr>
                    </thead>
                    <tbody>

                        <% services.forEach(function(service) { %>
                            <tr>
                                <td>
                                    <%= service.nameService %>
                                </td>
                                <td>
                                    <%= service.description %>
                                </td>
                            </tr>
                            <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>